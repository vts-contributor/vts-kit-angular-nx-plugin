{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "SchematicsAngularEnhance",
  "title": "Angular Enhance Schema",
  "cli": "nx",
  "type": "object",
  "description": "Enhance default workspace.",
  "additionalProperties": false,
  "properties": {
    "style": {
      "description": "The file extension or preprocessor to use for style files, or `none` to skip generating the style file.",
      "type": "string",
      "default": "scss",
      "enum": [
        "scss",
        "css",
        "less"
      ]
    },
    "defaultApp": {
      "description": "Setting default app for enhancement",
      "type": "string"
    }
  },
  "required": []
}